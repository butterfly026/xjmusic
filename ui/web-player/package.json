{
  "name": "web-player",
  "version": "0.0.1",
  "description": "XJ Music open source web player",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:xjmusic/web-player.git"
  },
  "scripts": {
    "start": "gulp watch",
    "build": "gulp"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^js(.*)$": "<rootDir>/src/js/$1"
    },
    "setupFiles": [
      "<rootDir>/test-env.js"
    ]
  },
  "author": "N. Charney Kaye",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-jest": "^24.7.1",
    "browser-sync": "2.26.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "gulp": "^4.0.1",
    "gulp-autoprefixer": "6.1.0",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-filter": "^5.1.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "5.0.3",
    "gulp-plumber": "1.2.1",
    "gulp-sass": "4.0.2",
    "gulp-uglify": "3.0.2",
    "jest": "^24.7.1",
    "pump": "3.0.0"
  }
}
