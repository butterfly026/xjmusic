<div class="meme-usage">

  <div class="name">{{name}}</div>

  {{#each model.programsWithMeme as |program|}}
    {{#if program.programHasMeme}}
      <div class="program">
        {{#link-to "go.program" program.programId}}
          <small>{{program.programType}}-Program #{{program.programId}}</small>
          {{#if program.programName}}
            &quot;{{program.programName}}&quot;{{/if}}
        {{/link-to}}
      </div>
    {{/if}}
    {{#each program.sequencesWithMeme as |sequence|}}
      <div class="program-sequence">
        {{#link-to "go.program" program.programId}}
          <small>{{program.programType}}-Program #{{program.programId}}</small>
          {{#if program.programName}}
            &quot;{{program.programName}}&quot;{{/if}}
        {{/link-to}}
        <small>{{sequence.sequenceType}}-Sequence #{{sequence.sequenceId}}</small>
        {{#if sequence.sequenceName}}
          &quot;{{sequence.sequenceName}}&quot;{{/if}}
        {{#if sequence.programBindingOffset}}
          @{{sequence.programBindingOffset}}{{/if}}
      </div>
    {{/each}}
  {{/each}}

  {{#each model.instrumentsWithMeme as |instrument|}}
    <div class="instrument">
      {{#link-to "go.instrument" instrument.instrumentId}}
        <small>{{instrument.instrumentType}}-Instrument #{{instrument.instrumentId}}</small>
        {{#if instrument.instrumentDescription}}
          &quot;{{instrument.instrumentDescription}}&quot;{{/if}}
      {{/link-to}}
    </div>
  {{/each}}

  {{yield}}
</div>