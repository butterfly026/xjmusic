<!-- Parent (Instrument) -->
<div class="form-group">
  <label class="col-sm-2 control-label">Instrument</label>
  <div class="col-sm-10">
    <div class="readonly-field">{{model.instrument.name}}</div>
  </div>
</div>

<!-- Name -->
<div class="form-group">
  <label class="col-sm-2 control-label">Name</label>
  <div class="col-sm-10">
    {{input type="text" value=model.name class="form-control" autofocus="autofocus"
            placeholder="The name of the Audio"}}
  </div>
</div>

<!-- WaveformUrl -->
<div class="form-group">
  <label class="col-sm-2 control-label">Waveform URL</label>
  <div class="col-sm-10">
    {{input type="text" value=model.waveformUrl class="form-control" autofocus="autofocus"
            placeholder="The Waveform URL of the Audio"}}
  </div>
</div>

<!-- Start -->
<div class="form-group">
  <label class="col-sm-2 control-label">Start (Seconds)</label>
  <div class="col-sm-10">
    {{input type="number" step="any" value=model.start class="form-control"
            placeholder="The start of the Audio"}}
  </div>
</div>

<!-- Length -->
<div class="form-group">
  <label class="col-sm-2 control-label">Length (Seconds)</label>
  <div class="col-sm-10">
    {{input type="number" step="any" value=model.length class="form-control"
            placeholder="The length of the Audio"}}
  </div>
</div>

<!-- Pitch -->
<div class="form-group">
  <label class="col-sm-2 control-label">Pitch (Hz)</label>
  <div class="col-sm-10">
    {{input type="number" step="any" value=model.pitch class="form-control"
            placeholder="The pitch of the Audio"}}
  </div>
</div>

<!-- Tempo -->
<div class="form-group">
  <label class="col-sm-2 control-label">Tempo (BPM)</label>
  <div class="col-sm-10">
    {{input type="number" step="any" value=model.tempo class="form-control"
            placeholder="The tempo of the Audio"}}
  </div>
</div>

