<!-- Inner Content -->
{{outlet}}

<!-- Copyright (c) 2017, Outright Mental Inc. (https://w.outright.io) All Rights Reserved. -->

<!-- Upper Container -->
<div class="form-horizontal">

  <!-- List of Links -->
  <div class="sm-col-12">
    <div class="table-list">
      <table class="table">
        <thead>
        <tr>
          <th width="1%">Offset</th>
          <th width="1%">State</th>
          <th>Begin At</th>
          <th>End At</th>
          <th>Messages</th>
          <th>Total</th>
          <th>Density</th>
          <th>Key</th>
          <th>Tempo</th>
          <th>#</th>
        </tr>
        </thead>
        <tbody>
        {{#each model.links as |link|}}
          <tr class="item {{link.state}}-state">
            <td>{{#link-to "accounts.one.chains.one.links.one" link}}{{link.offset}}{{/link-to}}</td>
            <td>{{link.state}}</td>
            <td>{{link.beginAt}}</td>
            <td>{{link.endAt}}</td>
            <td>{{message-badges link}}</td>
            <td>{{link.total}}</td>
            <td>{{link.density}}</td>
            <td>{{link.key}}</td>
            <td>{{link.tempo}}</td>
            <th scope="row">{{link.id}}</th>
          </tr>
        {{/each}}
        </tbody>
      </table>
    </div>
  </div>

</div>
