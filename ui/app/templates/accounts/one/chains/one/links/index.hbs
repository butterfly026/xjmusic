<!-- Inner Content -->
{{outlet}}

<!-- Copyright (c) 2017, Outright Mental Inc. (https://w.outright.io) All Rights Reserved. -->

<!-- Upper Container -->
<div class="form-horizontal">

  <!-- List of Links -->
  <div class="sm-col-12">
    <div class="table-list">
      <table class="table">
        <thead>
        <tr>
          <th width="1%">Offset</th>
          <th width="1%">State</th>
          <th>End At</th>
          <th>Total</th>
          <th>Density</th>
          <th>Key</th>
          <th>Tempo</th>
          <th>Memes</th>
          <th>Choices</th>
          <th>Chords</th>
          <th>Messages</th>
        </tr>
        </thead>
        <tbody>
        {{#each model.links as |link|}}
          <tr class="item {{link.state}}-state">
            <th>{{#link-to "accounts.one.chains.one.links.one" link}}{{link.offset}}{{/link-to}}</th>
            <td>{{link.state}}</td>
            <td>{{link.endAt}}</td>
            <td>{{link.total}}</td>
            <td>{{link.density}}</td>
            <td>{{link.key}}</td>
            <td>{{link.tempo}}</td>
            <td>{{link-memes link}}</td>
            <td>{{link-choices link}}</td>
            <td>{{link-chords link}}</td>
            <td>{{message-badges link}}</td>
          </tr>
        {{/each}}
        </tbody>
      </table>
    </div>
  </div>

</div>
