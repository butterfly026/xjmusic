#!/usr/bin/env bash

# Include common functions
. $( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/common/functions

# apps, configs, static assets
HUB_BASEURL="http://localhost/api/1"
HEARTBEAT_KEY="1234"

curl -v --data "key=${HEARTBEAT_KEY}" -H "Content-Type: application/x-www-form-urlencoded" "${HUB_BASEURL}/heartbeat" | jq .
