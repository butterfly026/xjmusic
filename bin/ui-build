#!/usr/bin/env bash

# Include common functions
. $( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/common/functions

#
step "Build Hub UI"
cd ui/hub-ui || exit 1
npm install || exit 1
bower install || exit 1
ember test || exit 1
ember build --environment=production || exit 1
chmod 755 -R dist/assets
step_ok

#
finished_ok
